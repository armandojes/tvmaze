{
  "name": "tvmaze-ssr",
  "alias": "tvmaze-ssr",
  "version": 2,
  "builds": [
    { "src": "./build/server.js", "use": "@now/node-server" },
    { "src": "./public/*", "use": "@now/static" },
    { "src": "./public/img/*", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/public/(.*)","dest": "/public/$1" },
    { "src": "(.*)", "dest": "/build/server.js" }
  ]
}
